<div class="background-start">
  <div class="container">
    <div class="row">
      <h1 class="logo3">Trippin</h1>
      <div class="col-sm-5 col-sm-offset-3 white"><br><br><br><br>
          <h1>Login</h1>
          <%= bootstrap_form_for(:user) do |f| %>
            <%= f.text_field :email %>
            <%= f.password_field :password %>
            <%= f.submit 'login', class: "btn btn-info" %>
          <% end %>
      </div>
    </div>
  </div>
</div>